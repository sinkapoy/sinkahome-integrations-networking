{
    "name": "@sinkapoy/home-integrations-networking",
    "version": "0.0.1",
    "description": "networking module for sinkahome",
    "main": "./dist/node.js",
    "browser": "./dist/browser.js",
    "typings": "./dist/index.d.ts",
    "type": "module",
    "author": "sinkapoy",
    "license": "GPL-3.0-or-later",
    "scripts": {
